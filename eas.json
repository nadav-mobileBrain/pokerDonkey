{
  "cli": {
    "version": ">= 5.9.3"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios":{
        "resourceClass": "m-medium",
        "simulator": true
      } ,
      "env": {
        "GOOGLE_SERVICES_JSON": "./google-services.json"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "channel": "preview"
      },
      "env": {
        "GOOGLE_SERVICES_JSON": "@project:GOOGLE_SERVICES_JSON"
      }
    },
    "production": {
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
